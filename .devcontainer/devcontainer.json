{
	"name": "Metafiles-dev",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.shell.linux": "/bin/bash"
			},
			"extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "GitHub.copilot",
				"GitHub.copilot-chat",
				"ms-python.mypy-type-checker"
			]
		}
	}
}